(define (problem multi-tower)
  (:domain blocksworld-4ops)
  (:objects A B C D E - block)
  (:init
    (arm-empty)
    (on-table A)
    (on-table B)
    (on-table C)
    (on-table D)
    (on-table E)
    (clear A)
    (clear B)
    (clear D)
    (clear E)
  )
  (:goal (and (on A C) (on-table C)
              (on B E) (on-table E)
              (on D B))))